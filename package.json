{
    "name": "litminka-api",
    "version": "2.0.0-alpha.1",
    "description": "api for the litminka service",
    "main": "index.js",
    "scripts": {
        "test": "jest --runInBand",
        "devmigrate": "prisma migrate dev",
        "migrate": "prisma migrate deploy",
        "build": "tspc && cp -r prisma dist/ && rm dist/prisma/seed.ts",
        "start": "node dist/src/index.js",
        "seed": "ts-node prisma/seed.ts",
        "dev": "npm install && npm run devmigrate && npx nodemon src/index.ts --inspect src/index.ts -r tsconfig-paths/register",
        "server": "npm install && npm run devmigrate && npx nodemon src/server.ts -r tsconfig-paths/register",
        "autocheck": "ts-node src/queues/autocheckWorker.ts",
        "relation-update": "ts-node src/queues/relationUpdateWorker.ts",
        "rating-update": "ts-node src/queues/ratingUpdateWorker.ts",
        "watchlist-importer": "ts-node src/queues/watchlistImportWorker.ts",
        "shikimori-watchlist-sync": "ts-node src/queues/shikimoriListUpdateWorker.ts",
        "lint": "eslint . --fix --cache",
        "format": "prettier . --write",
        "format:check": "prettier . --check"
    },
    "lint-staged": {
        "*": [
            "npx eslint . --cache"
        ]
    },
    "repository": {
        "type": "git",
        "url": "litminka-api"
    },
    "keywords": [
        "api",
        "litminka",
        "node",
        "js"
    ],
    "author": "Thenk0",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^5.11.0",
        "axios": "^1.6.7",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.20.2",
        "bullmq": "^3.6.2",
        "dayjs": "^1.11.7",
        "dotenv": "^16.0.3",
        "express": "^4.18.3",
        "express-slow-down": "^2.0.3",
        "express-validator": "^7.0.1",
        "follow-redirects": "^1.15.6",
        "helmet": "^5.1.0",
        "jsonwebtoken": "^9.0.0",
        "limiter": "^2.1.0",
        "node-fetch": "^2.6.7",
        "winston": "^3.12.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.2.1",
        "@commitlint/config-conventional": "^19.1.0",
        "@eslint/js": "^9.0.0",
        "@types/bcrypt": "^5.0.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/express": "^4.17.13",
        "@types/jest": "^29.5.12",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/node": "^18.0.1",
        "@types/node-fetch": "^2.6.2",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/parser": "^7.6.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "globals": "^15.0.0",
        "husky": "^8.0.0",
        "jest": "^29.7.0",
        "lint-staged": "^15.2.2",
        "nodemon": "^3.1.0",
        "prettier": "^3.2.5",
        "prisma": "^5.11.0",
        "supertest": "^6.3.4",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "ts-patch": "^3.1.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.4.5",
        "typescript-eslint": "^7.5.0",
        "typescript-transform-paths": "^3.4.7"
    }
}