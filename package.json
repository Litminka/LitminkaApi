{
    "name": "litminka-api",
    "version": "2.0.0-alpha.1",
    "description": "api for the litminka service",
    "main": "index.js",
    "scripts": {
        "test": "jest --runInBand",
        "devmigrate": "npx prisma migrate dev",
        "migrate": "npx prisma migrate deploy",
        "build": "npx tsc",
        "start": "npm run build && node dist/index.js",
        "seed": "npx ts-node prisma/seed.ts",
        "dev": "npm install && npm run devmigrate && npx nodemon --inspect src/index.ts -r tsconfig-paths/register",
        "autocheck": "npx ts-node src/queues/autocheck.ts",
        "relation-update": "npx ts-node src/queues/relationUpdate.ts",
        "rating-update": "npx ts-node src/queues/ratingUpdate.ts",
        "watchlist-importer": "npx ts-node src/queues/watchListWorker.ts",
        "shikimori-watchlist-sync": "npx ts-node src/queues/shikimoriListUpdate.ts",
        "lint": "npx eslint . --config eslint.config.mjs --fix",
        "format": "npx prettier src/**/*.ts --write",
        "format:check": "npx prettier src/**/*.ts --check",
        "prepare": "npx husky install"
    },
    "lint-staged": {
        "*": [
            "npm run format"
        ]
    },
    "repository": {
        "type": "git",
        "url": "litminka-api"
    },
    "keywords": [
        "api",
        "litminka",
        "node",
        "js"
    ],
    "author": "Thenk0",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^5.11.0",
        "axios": "^1.6.7",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.20.2",
        "bullmq": "^3.6.2",
        "dayjs": "^1.11.7",
        "dotenv": "^16.0.3",
        "eslint-config-love": "^46.0.0",
        "express": "^4.18.3",
        "express-validator": "^7.0.1",
        "follow-redirects": "^1.15.6",
        "form-data": "^4.0.0",
        "helmet": "^5.1.0",
        "jest": "^29.7.0",
        "jsonwebtoken": "^9.0.0",
        "limiter": "^2.1.0",
        "node-fetch": "^2.6.7",
        "ts-jest": "^29.1.2",
        "winston": "^3.12.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.2.1",
        "@commitlint/config-conventional": "^19.1.0",
        "@eslint/eslintrc": "^3.0.2",
        "@eslint/js": "^9.0.0",
        "@types/bcrypt": "^5.0.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/express": "^4.17.13",
        "@types/jest": "^29.5.12",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/node": "^18.0.1",
        "@types/node-fetch": "^2.6.2",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^7.6.0",
        "@typescript-eslint/parser": "^7.6.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-n": "^16.6.2",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-promise": "^6.1.1",
        "globals": "^15.0.0",
        "husky": "^8.0.0",
        "lint-staged": "^15.2.2",
        "nodemon": "^3.1.0",
        "prettier": "^3.2.5",
        "prisma": "^5.11.0",
        "supertest": "^6.3.4",
        "ts-node": "^10.8.2",
        "ts-node-dev": "^2.0.0",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^4.9.5",
        "typescript-eslint": "^7.5.0"
    }
}
