{
  "name": "litminka-api",
  "version": "1.0.0",
  "description": "api for the litminka service",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "devmigrate": "npx prisma migrate dev",
    "migrate": "npx prisma migrate deploy",
    "build": "npx tsc",
    "start": "npm run build && node dist/index.js",
    "seed": "npx ts-node prisma/seed.ts",
    "dev": "npx nodemon --inspect src/index.ts",
    "autocheck": "npx ts-node src/queues/autocheck.ts",
    "shikimoriupdate": "npx ts-node src/queues/shikimoriUpdate.ts",
    "watchlistimporter": "npx ts-node src/queues/watchListWorker.ts"
  },
  "repository": {
    "type": "git",
    "url": "litminka-api"
  },
  "keywords": [
    "api",
    "litminka",
    "node",
    "js"
  ],
  "author": "Thenk0",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "bullmq": "^3.6.2",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^5.0.0-beta.1",
    "express-validator": "^7.0.1",
    "follow-redirects": "^1.15.6",
    "form-data": "^4.0.0",
    "helmet": "^5.1.0",
    "jsonwebtoken": "^9.0.0",
    "limiter": "^2.1.0",
    "node-fetch": "^2.6.7",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.0.1",
    "@types/node-fetch": "^2.6.2",
    "nodemon": "^3.1.0",
    "prisma": "^5.10.2",
    "ts-node": "^10.8.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.7.4"
  }
}
